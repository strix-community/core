{
    "name": "strix/core",
    "type": "project",
    "description": "Strix is a powerful platform to handle donations, updates, and forums.",
    "keywords": [
        "strix",
        "laravel",
        "php",
        "livewire",
        "alpinejs"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "blade-ui-kit/blade-heroicons": "^0.2.1",
        "blade-ui-kit/blade-ui-kit": "^0.1.1",
        "brick/math": "^0.8.15",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hidehalo/nanoid-php": "^1.1",
        "igaster/laravel-theme": "^2.0",
        "laravel/fortify": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/socialite": "^v5.0.1",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "mews/purifier": "^3.3",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.9",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/steam": "^3.0",
        "spatie/laravel-medialibrary": "^8.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^4.0.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Strix\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --write"
        ]
    }
}