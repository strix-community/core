{
    "name": "strix/core",
    "type": "project",
    "description": "Strix is a powerful platform to handle donations, updates, and forums.",
    "keywords": [
        "strix",
        "laravel",
        "php",
        "livewire",
        "alpinejs"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "blade-ui-kit/blade-heroicons": "^0.2.1",
        "blade-ui-kit/blade-ui-kit": "^0.1.1",
        "brick/math": "^0.8.15",
        "hidehalo/nanoid-php": "^1.1",
        "igaster/laravel-theme": "^2.0",
        "laravel/fortify": "^1.4",
        "laravel/socialite": "^v5.0.1",
        "livewire/livewire": "^2.0",
        "mews/purifier": "^3.3",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.9",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/steam": "^3.0",
        "spatie/laravel-medialibrary": "^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
        "Strix\\": "src/",
        "Database\\Factories\\": "database/factories/",
        "Database\\Seeders\\": "database/seeders/"
        },
    "autoload-dev": {
        "psr-4": {
        "Tests\\": "tests/"
        }
    },
    "extra": {
    "laravel": {
        "providers": [
            "Strix\\Providers\\StrixServiceProvider"
        ],
    }
},
    "minimum-stability": "dev",
    "prefer-stable": true
    }
}
